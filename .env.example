# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Redirect URIs
# Using the same web redirect for both environments (simplifies Google OAuth setup)
# Only add this ONE URL to Google Cloud Console: http://localhost:3001/auth/google/success
DEVELOPMENT_REDIRECT_URI=http://localhost:3001/auth/google/success
PRODUCTION_REDIRECT_URI=http://localhost:3001/auth/google/success

# Which redirect URI to use (development or production)
NODE_ENV=development

# Your Auth Service Configuration
PORT=3001
AUTH_SERVICE_URL=http://localhost:3001

# Existing Ticketing API Configuration
TICKETING_API_BASE_URL=https://ticketing.development.atelier.ovh/api/mobile
TICKETING_REGISTER_ENDPOINT=/auth/register

# Security
JWT_SECRET=your_jwt_secret_here_for_state_tokens 